---
import AdvCalendar from "../../layouts/AdvCalendar.astro";
import { posts } from "./posts";

const internalPosts = posts.map((post) => ({
  title: post.title,
  publishedDate: post.publishedDate,
  url: post.url,
}));
---

<AdvCalendar
  year={2024}
  title="React Aria"
  posts={internalPosts}
  endDate={25}
/>
